{
  "hosting": {
    "public": "dist/apus-apus-dev/browser",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "**/*.json",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=60"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/uk-UA/**",
        "destination": "/uk-UA/index.html"
      },
      {
        "source": "/en-US/**",
        "destination": "/en-US/index.html"
      },
      {
        "source": "**",
        "destination": "/en-US/index.html"
      }
    ],
    "i18n": {
      "root": "/"
    }
  }
}
