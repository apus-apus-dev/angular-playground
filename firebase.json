{
  "hosting": {
    "public": "dist/apus-apus-dev/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/uk-UA/**",
        "destination": "/uk-UA/index.html"
      },
      {
        "source": "/en-US/**",
        "destination": "/en-US/index.html"
      },
      {
        "source": "**",
        "destination": "/en-US/index.html"
      }
    ],
    "i18n": {
      "root": "/"
    }
  }
}
